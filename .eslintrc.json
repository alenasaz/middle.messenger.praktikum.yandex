{
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "max-len": [2, 130],
      "@typescript-eslint/no-unused-vars": 2,
      "linebreak-style": 0,
      "import/extensions": ["error", "ignorePackages", {
        "js": "never",
        "ts": "never",
        "jsx": "never"
      }]
      }
  } 